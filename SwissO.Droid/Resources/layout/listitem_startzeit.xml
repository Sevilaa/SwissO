<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="2dp"
    android:paddingBottom="2dp"
    android:paddingEnd="5dp">

    <TextView
        android:text=""
        android:layout_width="45dp"
        android:layout_height="wrap_content"
        android:gravity="end"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/sl_name"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/sl_startnummer" />
    
    <TextView
        android:text=""
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingStart="5dp"
        app:layout_constraintStart_toEndOf="@id/sl_startnummer"
        app:layout_constraintEnd_toStartOf="@id/sl_kat"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/sl_name" />
    <TextView
        android:text=""
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:paddingStart="5dp"
        app:layout_constraintStart_toEndOf="@id/sl_name"
        app:layout_constraintEnd_toStartOf="@id/sl_starttime"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/sl_kat" />
    <TextView
        android:text=""
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:gravity="end"
        android:paddingStart="5dp"
        app:layout_constraintStart_toEndOf="@id/sl_kat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/sl_starttime" />
    
    <!--<TextView
        android:text=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        app:layout_constraintStart_toEndOf="@id/rl_starttime"
        app:layout_constraintEnd_toStartOf="@id/rl_club"
        android:id="@+id/rl_jahrgang" />

    <TextView
        android:text=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/rl_jahrgang"
        android:id="@+id/rl_club" />-->

</androidx.constraintlayout.widget.ConstraintLayout>
